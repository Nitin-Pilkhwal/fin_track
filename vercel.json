{
    "version": 2,
    "builds": [
        {
            "src": "finance_project/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb",
                "runtime" : "python3.11.9",
                "buildCommand": "bash setup.sh"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "finance_project/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}